{% extends "generic_prod.html" %}
{% block style-added %}
    <link href="../static/css/bootstrapSwitch.css" rel="stylesheet">
    <style type="text/css">
        p {
            display: inline;
            vertical-align: middle;
        }

        h2 {
            display: inline;
            vertical-align: middle;
        }

        h3 {
            display: inherit;
        }

        h4 {
            display: inline;
            vertical-align: middle;
            padding-right: 15px;
        }

        h5 {
            margin: 1px;
        }

