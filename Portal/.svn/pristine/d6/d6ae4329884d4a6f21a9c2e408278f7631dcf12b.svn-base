<script>
       $(function () {

    var colors = Highcharts.getOptions().colors,
        categories = ['Wind OS', 'BAO', 'MAP', 'EAI'],
        name = 'envision',
        data = [{
                y: 510,
                color: colors[0],
                drilldown: {
                    name: 'Wind OS',
                    categories: ['SCADA_APP', 'FE', 'HDB', 'WebClient'],
                    data: [130, 200,100, 80],
                    color: colors[0]
                }
            }, {
                y: 402,
                color: colors[4],
                drilldown: {
                    name: 'BAO',
                    categories: ['Web', 'ETL'],
                    data: [109, 293],
                    color: colors[4]
                }
            }, {
                y: 304,
                color: colors[2],
                drilldown: {
                    name: 'MAP',
                    categories: ['IOS', 'Android'],
                    data: [20,284],
                    color: colors[2]
                }
            }, {
                y: 109,
                color: colors[3],
                drilldown: {
                    name: 'EAI',
                    categories: ['Web', 'ETL'],
                    data: [29, 80],
                    color: colors[3]
                }
            }];

    function setChart(name, categories, data, color) {
	chart.xAxis[0].setCategories(categories, false);
	chart.series[0].remove(false);
	chart.addSeries({
		name: name,
		data: data,
		color: color || 'white'
	}, false);
	chart.redraw();
    }

    var chart = $('#container2').highcharts({
        chart: {
            type: 'column'
        },
        title: {
            text: '自动化用例数',
            style:{
                fontSize:'25px',
                color: 'black'
            }
        },
        credits:{
            enabled: false
        },
        exporting:{
            enabled: true
        },
        xAxis: {
            categories: categories
        },
        yAxis: {
            title: {
                text: 'Total Number',
                style:{
                fontSize:'16px',
                color: 'black'
                    }
            }
        },
        plotOptions: {
            column: {
                cursor: 'pointer',
                point: {
                    events: {
                        click: function() {
                            var drilldown = this.drilldown;
                            if (drilldown) { // drill down
                                setChart(drilldown.name, drilldown.categories, drilldown.data, drilldown.color);
                            } else { // restore
                                setChart(name, categories, data);
                            }
                        }
                    }
                },
                dataLabels: {
                    enabled: true,
                    color: colors[0],
                    style: {
                        fontWeight: 'bold'
                    },
                    formatter: function() {
                        return this.y;
                    }
                }
            }
        },
        tooltip: {
            formatter: function() {
                var point = this.point,
                    s = this.x +':<b>'+ this.y +'</b><br/>';
                if (point.drilldown) {
                    s += 'Click to detailed information of '+ point.category;
                } else {
                    s += 'Click to return to envision';
                }
                return s;
            }
        },
        series: [{
            name: name,
            data: data,
            color: 'white'
        }],

        exporting: {
            enabled: true
        }
    })
    .highcharts(); // return chart
});
</script>

