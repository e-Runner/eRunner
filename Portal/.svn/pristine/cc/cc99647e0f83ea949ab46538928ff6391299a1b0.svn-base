       }
       function FailedFilter()
        {
            var FailedJob_json = {

					JobIndex: "{{ JobIndex }}"
				};

		    var json_str = JSON.stringify(FailedJob_json);
            $.post("/SearchFailedResult/",json_str, function(data){
            $("#TableShow tbody").html("");
